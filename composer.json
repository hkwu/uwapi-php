{
    "name": "hkwu/uwapi-php",
    "description": "PHP wrapper for the University of Waterloo's Open Data API.",
    "keywords": [
        "uwaterloo api",
        "university of waterloo"
    ],
    "homepage": "https://github.com/hkwu/uwapi-php",
    "license": "MIT",
    "authors": [
        {
            "name": "Kelvin Wu",
            "email": "kelvinwu1996@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "guzzlehttp/guzzle": "^6.2",
        "nicmart/string-template": "^0.1.0",
        "symfony/options-resolver": "^3.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.4",
        "vlucas/phpdotenv": "^2.3",
        "fzaninotto/faker": "^1.6"
    },
    "autoload": {
        "psr-4": {
            "UWaterlooAPI\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }
}
